{
    "appId": "com.react.app",
    "asar": true,
    "directories": {
        "output": "dist/built"
    },
    "files": [
        "dist/**/*",
        "!public/**/*",
        "!dist/**/*.map",
        "!node_modules",
        "!**/*.md",
        "!**/test/**/*",
        "!**/README.md",
        "!**/.git/**/*",
        "!**/.github/**/*",
        "!dist/built"
    ],
    "extraResources": [],
    "mac": {
        "target": [
            "dmg",
            "zip"
        ]
    },
    "win": {
        "target": "nsis"
    }
}